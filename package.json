{
  "name": "rfc-state",
  "version": "1.0.0",
  "description": "a new way to write react function component",
  "main": "dist/cjs/index.js",
  "module": "dist/index.js",
  "scripts": {
    "build": "tsc --module es6 --outDir dist && tsc --module commonjs --outDir dist/cjs",
    "test": "jest --reporter=lcov --coverage",
    "jest": "jest --collect-coverage",
    "coverage": "codecov --disable=gcov,search -t 990bb158-3ffc-40d4-8803-b21075f074f1 -f coverage/coverage-final.json",
    "link": "npm link && npm link rfc-state"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elvinzhu/rfc-state.git"
  },
  "keywords": [
    "react",
    "component",
    "state",
    "callback"
  ],
  "author": "elvin zhu",
  "files": [
    "dist",
    "types",
    "effects",
    "index.d.ts",
    "effects.d.ts"
  ],
  "license": "MIT",
  "sideEffects": false,
  "types": "./index.d.ts",
  "bugs": {
    "url": "https://github.com/elvinzhu/rfc-state/issues"
  },
  "homepage": "https://github.com/elvinzhu/rfc-state/blob/main/README.md",
  "devDependencies": {
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@types/jest": "^27.0.3",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "codecov": "^3.8.3",
    "jest": "^27.4.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "tslib": "^2.3.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/*.ts"
    ]
  }
}
